{
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": false,
    "importOrder": [
        "^react(.*)",
        "antd/(.*)",
        "<THIRD_PARTY_MODULES>",
        "@/(.*)",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "plugins": ["prettier-plugin-tailwindcss"]
}
